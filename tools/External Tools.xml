      <option name="COMMAND" value="$JDKPath$\orbd.exe" />
      <option name="PARAMETERS" value="-ORBInitialPort 61101 -ORBInitialHost localhost" />
      <option name="WORKING_DIRECTORY" />
    </exec>
  </tool>
  <tool name="OBRD LVL" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
