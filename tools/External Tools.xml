      <option name="COMMAND" value="$JDKPath$\orbd.exe" />
      <option name="PARAMETERS" value="-ORBInitialPort 51101 -ORBInitialHost localhost" />
      <option name="WORKING_DIRECTORY" />
    </exec>
  </tool>
</toolSet>